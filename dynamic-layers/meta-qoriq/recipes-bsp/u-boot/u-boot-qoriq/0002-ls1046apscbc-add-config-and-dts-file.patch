+++ b/arch/arm/dts/Makefile
@@ -621,6 +621,7 @@ dtb-$(CONFIG_FSL_LSCH2) += fsl-ls1043a-qds-duart.dtb \
 	fsl-ls1046a-qds-lpuart.dtb \
 	fsl-ls1046a-rdb.dtb \
 	fsl-ls1046a-pscb.dtb \
+	fsl-ls1046a-pscbc.dtb \
